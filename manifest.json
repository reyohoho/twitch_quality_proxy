{
  "manifest_version": 3,
  "name": "ReYohoho Twitch Proxy",
  "version": "1.1",
  "description": "Proxies Twitch usher requests to a custom proxy.",
  "permissions": [
    "declarativeNetRequest",
    "webNavigation",
    "webRequest"
  ],
  "host_permissions": [
    "https://usher.ttvnw.net/*",
    "https://*.twitch.tv/*",
    "https://proxy4.rhhhhhhh.live/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
} 